@startuml Action
skinparam monochrome true

title Action

partition mousedown {
	:_event: MouseEvent]
	stop
}

partition mousemove {
	:_event: MouseEvent]
	stop
}

partition mouseup {
	:_event: MouseEvent]
	stop
}

partition prequisitesFulfilled {
	start
	:true]
	detach
}

partition additionalDisplay {
	:_crc: CanvasRenderingContext2D]
	stop
}

@enduml